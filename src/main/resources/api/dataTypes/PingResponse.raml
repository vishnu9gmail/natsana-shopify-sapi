#%RAML 1.0 DataType

type: object
properties:
  name: 
    type: string
    required: true
    example: "Shopify Sys API"
  tier:
    type: string
    required: true
    example: "System API"
  status:
    type: string
    required: true
    example: "UP"
  timestamp:
    type: datetime  
    required: true
    example: 2016-02-28T16:41:41.090Z
  hostname:
    type: string
    required: true
    example: "Shopify Sys API"
  